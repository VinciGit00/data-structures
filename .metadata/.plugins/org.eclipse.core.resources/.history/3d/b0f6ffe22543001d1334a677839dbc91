package Es2;

import java.lang.reflect.Array;
import java.util.ArrayList;
import java.util.Iterator;
import java.util.ListIterator;

public class DuplicatedObject implements duplicati {

	@Override
	public boolean verificaDupForLoop(int[] array) {
		for (int i = 0; i < array.length; i++) {
			for (int j = i + 1; j < array.length; j++) {
				if (array[i] == array[j])
					return true;
			}
		}
		return false;
	}
	
	@Override
	public boolean verificaDupForLoopArrayList(ArrayList arrayList) {
		for (int i = 0; i < arrayList.size(); i++) {
			for (int j = i + 1; j < arrayList.size(); j++) {
				if (arrayList.get(i).equals(arrayList.get(j)))
					return true;
			}
		}
		return false;
	}

	@Override
	public boolean verificaDupIterator(Iterator it) {
		while (it.hasNext()) {
			
			Iterator next = (Iterator) it.next();
			i = it
			while(next.hasNext()) {
				
			}
		
		}
		return false;
	}

	@Override
	public boolean verificaDupListIterator(ListIterator it) {
		// TODO Auto-generated method stub
		return false;
	}

	

}
