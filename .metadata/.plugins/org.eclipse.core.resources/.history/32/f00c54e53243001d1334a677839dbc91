package Es2;

import java.util.ArrayList;
import java.util.Iterator;
import java.util.LinkedList;
import java.util.ListIterator;

public interface duplicati {
	<T> boolean verificaDupForLoop(T []array);
	
	boolean verificaDupForLoopArrayList(ArrayList arrayList);
	
	boolean verificaDupForLoopLinkedList(LinkedList arrayList);
	
	 <T>  boolean verificaDupIterator(ArrayList arrayList );
	
	boolean verificaDupListIterator(ListIterator it);
}
