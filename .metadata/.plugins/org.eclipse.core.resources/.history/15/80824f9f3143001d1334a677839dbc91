package Es2;

import java.lang.reflect.Array;
import java.util.ArrayList;
import java.util.Iterator;
import java.util.LinkedList;
import java.util.ListIterator;

public class DuplicatedObject implements duplicati {

	@Override
	public boolean verificaDupForLoop(int[] array) {
		for (int i = 0; i < array.length; i++) {
			for (int j = i + 1; j < array.length; j++) {
				if (array[i] == array[j])
					return true;
			}
		}
		return false;
	}

	@Override
	public boolean verificaDupForLoopArrayList(ArrayList arrayList) {
		for (int i = 0; i < arrayList.size(); i++) {
			for (int j = i + 1; j < arrayList.size(); j++) {
				if (arrayList.get(i).equals(arrayList.get(j)))
					return true;
			}
		}
		return false;
	}

	@Override
	public boolean verificaDupForLoopLinkedList(LinkedList linkedList) {
		for (int i = 0; i < linkedList.size(); i++) {
			for (int j = i + 1; j < linkedList.size(); j++) {
				if (linkedList.get(i).equals(linkedList.get(j)))
					return true;
			}
		}
		return false;
	}

	@Override
	public <T> boolean verificaDupIterator(ArrayList arrayList) {
		Iterator <T> it = arrayList.listIterator();
		while (it.hasNext()) {
		 T i = it.next();
			arrayList.remove(0);
			Iterator<T> at = arrayList.listIterator();
			while (at.hasNext()) {
				T j =  at.next();
				if (i.equals(j)) {
					return true;
				}
			}

		}
		return false;
	}

	@Override
	public boolean verificaDupListIterator(ListIterator it) {
		// TODO Auto-generated method stub
		return false;
	}

}
