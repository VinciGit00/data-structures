package Objects;

//Only student's status information
public class Student implements Comparable<Student> {

	String Name;
	String Surname;
	String University;
	String Birthday;
	int Code;
	boolean Exchange;

	public Student(String Name, String Surname, String University, String Birthday, int Code, boolean Exchange) {
		this.Name = Name;
		this.Surname = Surname;
		this.University = University;
		this.Birthday = Birthday;
		this.Code = Code;
		this.Exchange = Exchange;
	}

	public boolean Equals(Object o) {
		if(this == o) return true;
		if (!(o instanceof Student)) return false;
		Student temp = (Student) o;
		return Name.equals(temp.Name) && Surname.equals(temp.Surname) &&
				University.equals(temp.University) && Birthday.equals(temp.Birthday) &&
				Code == temp.Code && Boolean.compare(Exchange, temp.Exchange);
	}

	@Override
	public int compareTo(Student o) {
		// TODO Auto-generated method stub
		return 0;
	}
}
